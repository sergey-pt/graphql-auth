{
  "name": "client",
  "version": "1.0.0",
  "description": "My gnarly Nuxt.js project",
  "author": "Sergei Anisimov",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest --watch --runInBand --silent",
    "test-debugger": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "jest": "jest --ci --runInBand --silent --reporters=default --reporters=jest-junit",
    "heroku-postbuild": "npm run build",
    "heroku-deploy": "cd ../ && git subtree push --prefix client heroku-client master || true"
  },
  "jest": {
    "reporters": [
      "default",
      "jest-junit"
    ],
    "verbose": true
  },
  "dependencies": {
    "@nuxtjs/dotenv": "^1.3.0",
    "apollo-boost": "^0.4.1",
    "cross-env": "^5.2.0",
    "cross-fetch": "^3.0.4",
    "graphql": "^14.4.1",
    "js-cookie": "^2.2.0",
    "jwt-decode": "^2.2.0",
    "nuxt": "^2.8.1",
    "nuxt-purgecss": "^0.2.1",
    "tailwindcss": "^1.0.4",
    "vuelidate": "^0.7.4"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.7.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "jest": "^24.8.0",
    "jest-junit": "^6.4.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "vue-jest": "^3.0.3"
  }
}
